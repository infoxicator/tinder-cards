    "concurrently": "^3.6.1",
    "cors": "2.8.4",
    "express": "4.16.3",
    "express-jwt": "5.3.1",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.1.1",
    "jsonwebtoken": "8.2.0",
    "notarealdb": "0.2.2"
  },
  "devDependencies": {
    "nodemon": "1.17.1"
  }
}
